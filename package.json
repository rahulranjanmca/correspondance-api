  "scripts": {
    "archive": "npm run gulp-archive",
    "build": "npm run lint && npm run gulp-build",
    "depcheck": "npm-check --production --color",
    "gulp-archive": "gulp archive",
    "gulp-build": "gulp build",
    "gulp-publish": "gulp publish",
    "lint": "eslint src/api/**/*.ts --fix --color && prettier --write src/api/**/*.ts",
    "modclean": "modclean --run && npm run npx-tailor",
    "npx-tailor": "npx dt-oneagent-tailor --AwsLambdaV8",
    "precommit": "npm run lint",
    "start": "cross-env NODE_ENV=LOCAL node -r ts-node/register src/index.ts",
    "dev": "cross-env NODE_ENV=LOCAL node -r ts-node/register src/index.ts",
    "test": "jest",
    "test:cov": "jest --coverage",
    "mock": "node -r ts-node/register mock/main.ts",
    "import": "node -r ts-node/register tool/import.ts",
    "heroku-postbuild": "echo Skip build on Heroku"
  },
